{{- $v := .Values.charts | get "pullsecret-patcher" dict }}
registry:
  username: {{ $v.username }}
  password: {{ $v.password }}
  {{- with $v | get "server" nil }}
  server: {{ . }}
  {{- end }}
  {{- with $v | get "email" nil }}
  email: {{ . }}
  {{- end }}
env:
  CONFIG_DEBUG: true
{{- with $v | get "env" nil }}   
{{- . | toYaml | nindent 2 }}
{{- end }}